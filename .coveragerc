[run]
parallel = True

omit =
    # used only for debugs purposes
    prototype/parser/CST.py
    prototype/parser/Utils.py
    # auto-generated files
    prototype/parser/Prototype*.py

source =
    prototype

[report]
exclude_lines =
    # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError
    raise ValueError

